{
  "objective": "binary:logistic",
  "eval_metric": ["auc", "logloss"],
  "booster": "gbtree",
  "eta": 0.05,
  "max_depth": 6,
  "min_child_weight": 1,
  "subsample": 0.8,
  "colsample_bytree": 0.8,
  "alpha": 0.1,
  "lambda": 1.0,
  "scale_pos_weight": 3.0,
  "max_delta_step": 1,
  "tree_method": "hist",
  "grow_policy": "lossguide",
  "max_leaves": 32,
  "max_bin": 256,
  "early_stopping_rounds": 10,
  "num_boost_round": 500,
  "nthread": -1,
  "verbosity": 1,
  "feature_importance_type": "gain",
  "feature_selector": "cyclic",
  "feature_groups": {
    "demographics": ["age", "gender", "race", "ethnicity", "insurance_type"],
    "comorbidities": [
      "diabetes",
      "hypertension",
      "heart_failure",
      "copd",
      "ckd",
      "cancer"
    ],
    "utilization": [
      "previous_admissions",
      "previous_ed_visits",
      "previous_outpatient_visits"
    ],
    "medications": [
      "medication_count",
      "high_risk_medications",
      "adherence_score"
    ],
    "labs": ["abnormal_lab_count", "lab_trend_features"]
  },
  "calibration": {
    "method": "isotonic",
    "cv_folds": 5
  }
}
